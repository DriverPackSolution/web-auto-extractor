    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.2",
    "sinon": "^1.17.4",
    "standard": "^7.0.1"
  }
